////  TimelineNavigator//  Created by Ketan Anjaria on 2007-03-13.//  Copyright (c) 2007 __MyCompanyName__. All rights reserved.//package com.metaweb.samples.soundline {	import flash.display.*		import flash.text.*;	import flash.events.*;	import flash.net.*;		import metaliq.core.*	import metaliq.events.*;	public class TimelineNavigator extends Component {		public var list:DynamicList;		public var listMask:Sprite;		/**		 * Creates a new TimelineNavigator component instance.		 * com.metaweb.samples.soundline.TimelineNavigator		 */		public function TimelineNavigator() { 			super();		}				override protected function configUI() : void {			super.configUI();			list = new DynamicList();			addChild(list);			list.defaultItemClass = "com.metaweb.samples.soundline.TimelineNavigatorItem"			list.layout = DynamicList.LAYOUT_HORIZONTAL;			list.mask = listMask;			list.addEventListener(DynamicListEvent.ITEM_CLICK, onClickItem, false, 0, true);			var startYear:uint = 1900;			var endYear:uint = new Date().getFullYear() + 3;			for(var i:uint = startYear; i < endYear;i++) {				var dataModel:Object = {};				dataModel.date = new Date(i,0);				dataModel.label = String(i)				list.addItem(dataModel)			}			cacheAsBitmap = true;					}		private function onClickItem(p_event:Event):void{			dispatchEvent(p_event) 		}		override public function draw() : void {			super.draw();		}			}}